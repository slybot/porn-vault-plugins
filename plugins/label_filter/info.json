{
  "name": "label_filter",
  "version": "0.0.1",
  "authors": ["boi123212321"],
  "description": "Filter labels returned by other plugins",
  "pluginEvents": [
    "actorCreated",
    "actorCustom",
    "sceneCreated",
    "sceneCustom",
    "movieCreated"
  ],
  "arguments": [
    {
      "name": "whitelist",
      "type": "Array<String>",
      "required": false,
      "default": [],
      "description": "Labels to include"
    },
    {
      "name": "blacklist",
      "type": "Array<String>",
      "required": false,
      "default": [],
      "description": "Labels to exclude"
    }
  ]
}
